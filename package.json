{
  "name": "which-package-manager",
  "version": "1.0.0",
  "keywords": [
    "pm",
    "package-manager",
    "yarn",
    "npm",
    "pnpm",
    "lockfile",
    "package-lock.json",
    "yarn.lock"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
